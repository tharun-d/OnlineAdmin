    <script>

        function validation() {

            var startdate = document.getElementById("one").value;
            var enddate = document.getElementById("two").value;
            var course = document.getElementById("three").value;
            document.getElementById("val").style.visibility = "hidden"

            if (startdate == "") {
                document.getElementById("val").innerText = "please select start date";
                document.getElementById("val").style.visibility = "visible"
                return false;
            }
            else if (enddate == "") {
                document.getElementById("val").innerText = "please select end date";
                document.getElementById("val").style.visibility = "visible"
                return false;
            }
            else if (course == "select") {
                document.getElementById("val").innerText = "please select course";
                document.getElementById("val").style.visibility = "visible"
                return false;
            }


            else {
                return true;
            }
        }
    </script>
